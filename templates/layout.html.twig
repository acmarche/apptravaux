{% extends 'base.html.twig' %}
{% block favicon %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
{% endblock %}
{% block title %}Interventions{% endblock %}

{% block brand %}
    <a class="navbar-brand" href="{{ path('homepage') }}" title="Accueil">
        <span class="glyphicon glyphicon-home"></span> Interventions </a>
{% endblock %}

{% block menu_top %}
    {% include 'menu.html.twig' %}
{% endblock %}

{% block notifications %}
    {% if app.user is not null and (is_granted("ROLE_TRAVAUX_VALIDATION") or (is_granted("ROLE_TRAVAUX_REDACTEUR"))) %}
        <div class="hidden-print">
            {{ render(controller('AcMarche\\Travaux\\Controller\\NotificationController::index')) }}
        </div>
    {% endif %}
{% endblock %}

{% block body %}

{% endblock %}